#tag Events Button_results
	#tag Event
		Sub Pressed()
		  
		  //Set active panel based on questions
		  If panasonic = True AND carbonite = True Then
		    Panel_Results.SelectedPanelIndex = 0
		  ElseIf panasonic = True AND carbonite = False Then
		    Panel_Results.SelectedPanelIndex = 1
		  ElseIf panasonic = False AND carbonite = True Then
		    Panel_Results.SelectedPanelIndex = 2
		  ElseIf panasonic = False AND carbonite = False AND ultrix = True Then
		    Panel_Results.SelectedPanelIndex = 3
		  ElseIf panasonic = False AND carbonite = False AND ultrix = False Then
		    Panel_Results.SelectedPanelIndex = 4
		  End
		  
		  //Turn on results panel
		  Panel_Results.Visible = True
		  Button_Restart.Visible = True
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Button5_opt1
	#tag Event
		Sub Pressed()
