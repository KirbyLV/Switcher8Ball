	#tag EndEvent
#tag EndEvents
#tag Events Button_Restart
	#tag Event
		Sub Pressed()
		  //Set panel visibility for restart
		  Panel_Questions.SelectedPanelIndex = 0
		  Panel_Results.Visible = False
		  Button_Restart.Visible = False
		  
		  //Reset starting variable values
		  panasonic = true
		  carbonite = true
		  ultrix = true
		  acuity = true
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
